#!/bin/sh
alias v="$EDITOR" \
    p="sudo pacman" \
    l="ls -a" \
    ls="ls -h --group-directories-first --color=auto" \
    ll="ls -la" \
    py="python3" \
    cfv="$EDITOR ~/.vimrc" \
    cfi="$EDITOR ~/.config/i3/config" \
    cfa="$EDITOR $SALTRICED/userconfig/_aliasrc && source $HOME/.bashrc" \
    cfb="$EDITOR ~/.bashrc && source $HOME/.bashrc" \
    cfp="$EDITOR ~/.bash_profile && source $HOME/.bashrc" \
    cft="cd /usr/src/st" \
    cdf="cd $SALTRICED"
